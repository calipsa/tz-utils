{
  "name": "@calipsa/tz-utils",
  "version": "0.3.4",
  "description": "JavaScript timezone utils",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "test": "jest --no-cache",
    "lint": "eslint './src/**/*.ts'",
