{
  "name": "@calipsa/tz-utils",
  "version": "1.0.5",
  "description": "JavaScript timezone utils",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "test": "jest --no-cache",
    "lint": "eslint './src/**/*.ts'",
    "build:commonjs": "tsc --outDir dist",
