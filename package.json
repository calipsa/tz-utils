    "tz",
    "timezone"
  ],
  "author": "Anton Veselev",
  "license": "ISC",
  "dependencies": {
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "moment-timezone": "^0.5.32"
  },
  "devDependencies": {
    "@calipsa/eslint-config-typescript": "^1.12.0",
    "@types/jest": "^26.0.15",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "np": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
