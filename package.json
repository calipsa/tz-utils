    "build": "rm -rf dist && run-p -c \"build:*\"",
    "version": "npm run build",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calipsa/tz-utils"
  },
  "keywords": [
    "js",
    "utils",
    "tz",
    "timezone"
  ],
  "author": "Anton Veselev",
  "license": "ISC",
  "dependencies": {
    "date-fns": "^2.19.0",
    "date-fns-tz": "^1.1.3",
    "moment-timezone": "^0.5.33"
  },
  "devDependencies": {
    "@calipsa/eslint-config-typescript": "^1.16.0",
    "@types/jest": "^26.0.20",
    "eslint": "^7.21.0",
    "jest": "^26.6.3",
    "np": "^7.4.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  }
}
