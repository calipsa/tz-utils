  ],
  "author": "Anton Veselev",
  "license": "ISC",
  "dependencies": {
    "moment-timezone": "^0.5.27",
    "@types/moment-timezone": "^0.5.12"
  },
  "devDependencies": {
    "@calipsa/eslint-config-typescript": "^1.2.1",
    "@types/jest": "^25.1.1",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "np": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  }
